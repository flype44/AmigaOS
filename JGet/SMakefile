
COMPILER=sc
OPTIONS=\
CODE=FAR\
DATA=FAR\
MATH=STANDARD\
NOCHECKABORT\
NOICONS\
NOSTACKCHECK\
STRINGMERGE\
STRIPDEBUG\
OPTIMIZE\
OPTIMIZERCOMPLEXITY=8\
OPTIMIZERDEPTH=5\
OPTIMIZERINLINELOCAL\
OPTIMIZERSCHEDULER\
OPTIMIZERTIME

OUTFILE=JGet

OBJECTS=JGet.o parson.o

all: $(OUTFILE)

clean:
	@delete $(OBJECTS)

$(OUTFILE): $(OBJECTS)
	$(COMPILER) $(OPTIONS) LINK $(OBJECTS)
